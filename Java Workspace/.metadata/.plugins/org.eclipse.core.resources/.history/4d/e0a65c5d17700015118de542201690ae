package velinov.java.finken.app.ui;

import javax.swing.JTabbedPane;

import velinov.java.finken.app.ui.tabs.ConnectionTab;


/**
 * defines the central view of the app.
 * 
 * @author Vladimir Velinov
 * @since Oct 11, 2015
 *
 */
public class CentralView extends JTabbedPane {

  private final ConnectionTab connectionTab;
  /**
   * default constructor.
   */
  public CentralView() {
    this.connectionTab = new ConnectionTab();
    
    this.addTab(this.connectionTab.getLabel(), this.connectionTab);
  }
}
